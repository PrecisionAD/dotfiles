# Source bash
alias so='cd ; source ~/.bash_aliases'

# Check temp
alias temp='sudo powermetrics --samplers smc | grep -i "CPU die temperature"'

# Go back 'n' directories
alias back='cd_back'

# Go to c++ directory
alias cpp='cd ~/hobbies/c++/'

# Go to c directory
alias goc='cd ~/hobbies/C'

# Open .vimrc
alias rc='vim ~/.vimrc'

# Freenas
alias freenas='ssh root@192.168.1.101'

# Run telefunke
alias tele='cd ~/hobbies/C/games ; ./telefunke'

#-------------------Git Stuff----------------------
alias stat='git status'
alias log='git log'


